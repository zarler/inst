/*网站公共样式*/

/*---页面头---*/
.t-login-nav {
    background: white;
    display: block;
    text-align: center;
    width: 100%;
    height: 2.25rem;
    line-height: 2.2rem;
    border-bottom: 1px solid #f7f2f2;
    position: fixed;
    z-index: 99;
}
.t-login-nav-1 {
    text-align: center;
    color: #222222;
    font-size: .95rem;
}
.return_i {
    background: url(/static/images/v1/icon_back.png) no-repeat;
    width: 1rem;
    height: 1rem;
    background-size: contain;
    display: inline-block;
    vertical-align: middle;
    margin-top: .56rem;
    left: .75rem;
    position: absolute;
}

.return_r {
    background: url(/static/images/v1/icon_jiantou.png) no-repeat;
    width: .8rem;
    height: .8rem;
    background-size: contain;
    /* display: inline-block; */
    vertical-align: middle;
    margin-top: .56rem;
    left: .75rem;
    float: right;
    margin-right: .4rem;
}

.t-login-nav-1 a {
    color: #363636;
}
/*---头顶空位符---*/
.t-top-vacancy{
    height: 2.25rem;
}

/*---表间隔---*/
.t-margintop-5{
    margin-top: .5rem;
}
/*---信息核对页---*/
.t-check-1 {
    background: white;
    color: #555555;
    padding: .6rem 1.1rem;
    font-size: .7rem;
    line-height: 1.1rem;
    font-weight: normal;
}

.t-check-1 label{
    color: #222222;
    font-weight: bolder;
}

.contractabroad{
    color: #9F9F9F;
    padding: .6rem 1.1rem;
    font-size: .65rem;
    line-height: 1.1rem;
}
.contractabroad a{
    color: #222222;
}
.submitButton{
    width: 90%;
    margin: 0 auto 1.4rem auto;
    display: block;
    height: 2rem;
    background: #0085EC;
    color: white;
    font-size: .8rem;
    border-radius: 1.8rem;
}

/*----首页----*/
.t-notice{
    background-color:rgba(58,58,58,0.2);
}
.t-notice p{
    color: #555555;
    text-align: left;
    font-size: .7rem;
    height: 1.8rem;
    line-height: 1.8rem;
    margin-left: .4rem;
}


.t-homepage {
    margin-top: 1.3rem;
    text-align: center;
}

.t-homepage .d1{
    margin: .2rem;
    text-align: center;
    font-size: .7rem;
    color: #9F9F9F;
}
.t-homepage .d2{
    text-align: center;
    font-size: 1.5rem;
    font-weight: 900;
    margin-right: 10%;
}
.t-homepage .d3{
    height: 5rem;
    width: 5rem;
    background: #0085EC;
    margin: 1.5rem auto 0 auto;
    border-radius: 8rem;
    line-height: 5rem;
    color: white;
    font-size: 18px;
    font-weight: 900;
    box-shadow: 0 0 0.8rem 0.2rem rgba(0, 133, 236, 0.32);
    display: block;
}